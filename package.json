{
  "name": "energy_saving_webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "7.1.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@popperjs/core": "^2.11.7",
    "@reduxjs/toolkit": "^1.9.5",
    "@tailwindcss/forms": "0.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "10.2.5",
    "axios": "^0.21.4",
    "connected-react-router": "^6.9.1",
    "craco-less": "^2.0.0",
    "dotenv-cli": "^5.1.0",
    "history": "^4.10.1",
    "postcss": "8.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-responsive-modal": "^6.4.1",
    "react-router-dom": "5.2.0",
    "react-script": "^2.0.5",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "redux-form": "^8.3.10",
    "tailwindcss": "^2.0.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "REACT_APP_ENV=local dotenv -e .env.local craco start",
    "dev": "REACT_APP_ENV=development dotenv -e .env.development craco start",
    "production": "REACT_APP_ENV=production dotenv -e .env.production craco start",
    "build": "craco build",
    "build-develop": "dotenv -e .env.development craco build",
    "build-production": "dotenv -e .env.production craco build",
    "test": "craco test",
    "eject": "craco eject",
    "eslint": "eslint --ignore-path .eslintignore --fix ./src/",
    "pretty-quick": "pretty-quick --pattern **/*.js --verbose",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,scss}": [
      "pretty-quick --staged",
      "prettier --write"
    ],
    "*.{js,jsx}": []
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.5.4",
    "babel-plugin-named-exports-order": "^0.0.2",
    "dotenv": "^6.2.0",
    "dotenv-cli": "^1.4.0",
    "eslint": "8.15.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "prop-types": "^15.8.1",
    "webpack": "^4.46.0"
  }
}
